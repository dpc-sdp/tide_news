{
    "name": "dpc-sdp/tide_news",
    "description": "News content type and related configuration for Tide Drupal 8 distribution",
    "type": "drupal-module",
    "license": "GPL-2.0+",
    "require": {
        "dpc-sdp/tide_core": "dev-master",
        "dpc-sdp/tide_media": "@dev",
        "drupal/metatag": "^1.5",
        "drupal/paragraphs": "^1.2",
        "drupal/smart_trim": "^1.1"
    },
    "suggest": {
        "dpc-sdp/tide_api": "dev-feature/SDPA-761-index-grants",
        "dpc-sdp/tide_webform": "Allows to use questionnaires"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "7.1"
        },
        "process-timeout": 0,
        "sort-packages": true
    }
}
